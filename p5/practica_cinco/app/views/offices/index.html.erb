<p id="notice"><%= notice %></p>

<h1>Offices</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |offiz| %>
      <tr>
        <td><%= link_to 'Show', offiz %></td>
        <td><%= link_to 'Edit', edit_offiz_path(offiz) %></td>
        <td><%= link_to 'Destroy', offiz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offiz', new_offiz_path %>
